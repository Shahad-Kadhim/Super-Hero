<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:right="14dp">
        <shape android:shape="rectangle">
            <solid android:color="#00000000" />
        </shape>
    </item>
    <item android:right="14dp" android:top="110dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
    <item android:left="0dp" >
       <rotate
           android:fromDegrees="14"
           android:toDegrees="37"
           android:pivotX="0%"
           android:pivotY="0%" >
           <shape android:shape="rectangle">
               <solid android:color="#FFFFFF" />
               <corners android:topLeftRadius="10dp" android:topRightRadius="4dp"/>
           </shape>

       </rotate>
   </item>




</layer-list>